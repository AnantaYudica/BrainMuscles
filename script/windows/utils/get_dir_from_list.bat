@ECHO OFF

SETLOCAL

SET FILENAME=
SET LIST_DIR=
SET CONJ=
SET RESULT=

SET FILENAME=%1
SET LIST_DIR=%2
SET CONJ=%3

SET FILENAME=%FILENAME:"=%
SET LIST_DIR=%LIST_DIR:"=%
SET CONJ=%CONJ:"=%

CALL :CHECK "%LIST_DIR%"
IF "%RESULT%"=="" (
	EXIT /B 1
	GOTO :END
)
ECHO %RESULT%
GOTO :END

:CHECK
IF NOT "%RESULT%"=="" (
	GOTO :END
)
SET LIST_DIR=%1
SET LIST_DIR=%LIST_DIR:"=%

FOR /F "tokens=1* delims=%CONJ%" %%A IN ("%LIST_DIR%") DO (
	CALL :IS_EXIST %%A
	CALL :CHECK "%%B"
)
GOTO :END

:IS_EXIST
IF EXIST "%1/%FILENAME%" (
	SET RESULT=%1
	GOTO :EOF
)
GOTO :END

:END
ENDLOCAL
